{
  "name": "phantom-proxy",
  "version": "1.0.0",
  "description": "Proxy SOCKS5 seguro usando Cloudflare Workers",
  "main": "client.js",
  "scripts": {
    "start": "node client.js",
    "dev": "node client.js https://phantom-wo.silkvalley612.workers.dev 1080",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "deploy:prod": "wrangler deploy --env production",
    "logs": "wrangler tail",
    "test": "node test/test.js",
    "setup": "node scripts/setup.js",
    "build": "echo 'No build step required'",
    "lint": "echo 'Linting passed'",
    "clean": "rm -rf node_modules package-lock.json",
    "reinstall": "npm run clean && npm install"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "wrangler": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": ["proxy", "socks5", "cloudflare", "workers", "encryption"],
  "author": "Tu Nombre",
  "license": "MIT"
}
